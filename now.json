{
  "version": 2,
  "name": "neu-web",
  "alias": "dev-web.pcwa.net",
  "public": false,
  "build": {
    "env": {
      "NPM_TOKEN": "@npm-token",
      "NEXT_MAILJET_URL": "https://dev-web.pcwa.net/lambda/mail",
      "NEXT_COSMIC_URL": "https://dev-web.pcwa.net/lambda/cosmic",
      "NEXT_TARGET": "serverless",
      "NEXT_RECAPTCHA_SITE_KEY": "@web-recaptcha-site-key",
      "NEXT_YOUTUBE_API_KEY": "@web-youtube-api-key",
      "NEXT_GOOGLE_MAPS_API_KEY": "@web-google-maps-api-key",
      "NEXT_PI_MAP_MAPBOX_API_KEY": "@web-pi-map-mapbox-api-key"
    }
  },
  "env": {
    "NODE_DARKSKY_API_KEY": "@web-darksky-api-key-dev",
    "NODE_REDISLABS_CACHE_PASSWORD": "@web-redislabs-cache-key",
    "NODE_MAILJET_KEY": "@web-mailjet-key",
    "NODE_MAILJET_SECRET": "@web-mailjet-secret",
    "NODE_MAILJET_SENDER": "webmaster@pcwa.net",
    "NODE_GOOGLE_CSE_CX": "@web-google-cse-cx",
    "NODE_GOOGLE_CSE_KEY": "@web-google-cse-key",
    "NODE_MAILCHIMP_DC": "us16",
    "NODE_MAILCHIMP_USERNAME": "pcwawater",
    "NODE_MAILCHIMP_API_KEY": "@web-mailchimp-api-key",
    "NODE_MAILCHIMP_PRIMARY_LIST": "fefab158d1",
    "NODE_DARKSKY_ROUTE_PREFIX": "/lambda/forecast",
    "NODE_MAILJET_ROUTE_PREFIX": "/lambda/mail",
    "NODE_COSMIC_ROUTE_PREFIX": "/lambda/cosmic",
    "NODE_GOOGLE_SEARCH_ROUTE_PREFIX": "/lambda/search",
    "NODE_MAILCHIMP_ROUTE_PREFIX": "/lambda/mc",
    "NODE_RECAPTCHA_SITE_KEY": "@web-recaptcha-site-key",
    "NODE_RECAPTCHA_SECRET_KEY": "@web-recaptcha-secret-key",
    "NODE_COSMIC_READ_ACCESS_KEY": "@web-cosmic-read-key",
    "NODE_COSMIC_WRITE_ACCESS_KEY": "@web-cosmic-write-key"
  },
  "builds": [
    {
      "src": "packages/www/package.json",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    { "src": "packages/api/**/*.ts", "use": "@now/node" },
    { "src": "packages/mailjet/dist/**/*.js", "use": "@now/node" },
    { "src": "packages/cosmic/dist/**/*.js", "use": "@now/node" },
    { "src": "packages/mailchimp/dist/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/api/forecast(.*)",
      "dest": "packages/api/forecast/index.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "GET"
      }
    },
    {
      "src": "/api/search(.*)",
      "dest": "packages/api/search/index.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "Access-Control-Allow-Origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/lambda/mail(.*)",
      "dest": "packages/mailjet/dist/index.js",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "Access-Control-Allow-Origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/lambda/cosmic(.*)",
      "dest": "packages/cosmic/dist/index.js",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "Access-Control-Allow-Origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/lambda/mc(.*)",
      "dest": "packages/mailchimp/dist/index.js",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "Access-Control-Allow-Origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/(?:typography)/?",
      "dest": "packages/www/_error",
      "status": 404
    },
    {
      "src": "/favicon.ico",
      "dest": "packages/www/static/favicon.ico",
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/packages/www/$1",
      "continue": true,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "x-request-path": "$1"
      }
    }
  ],
  "regions": ["sfo1"]
}

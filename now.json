      }
    },
    {
      "src": "/stewardship/stop-leaks",
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/smart-water-use/stop-leaks"
      }
    },
    {
      "src": "/newsroom/publication",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/newsroom/publications"
      }
    },
    {
      "src": "/newsroom/multimedia-library/p",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/multimedia-library/photos"
      }
    },
    {
      "src": "/newsroom/multimedia-library/v",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/multimedia-library/videos"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/packages/www/$1",
      "continue": true,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "x-request-path": "$1"
      }
    }
  ],
  "regions": ["sfo1"]
}

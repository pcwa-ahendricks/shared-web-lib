      }
    },
    {
      "src": "/newsroom/news-release",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/newsroom/news-releases"
      }
    },
    {
      "src": "/stewardship/?",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/smart-water-use"
      }
    },
    {
      "src": "/stewardship/water-sense",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/smart-water-use/watersense"
      }
    },
    {
      "src": "/stewardship/rebate-programs",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/smart-water-use/rebate-programs"
      }
    },
    {
      "src": "/stewardship/landscape-resources",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/smart-water-use/landscape-resources"
      }
    },
    {
      "src": "/stewardship/house-calls",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",

{
  "version": 2,
  "alias": "dev-web.pcwa.net",
  "public": false,
  "build": {
    "env": {
      "NEXT_TARGET": "serverless",
      "NEXT_RECAPTCHA_SITE_KEY": "@web-recaptcha-site-key",
      "NEXT_YOUTUBE_API_KEY": "@web-youtube-api-key",
      "NEXT_GOOGLE_MAPS_API_KEY": "@web-google-maps-api-key",
      "NEXT_PI_MAP_MAPBOX_API_KEY": "@web-pi-map-mapbox-api-key",
      "NEXT_DISTRICT_MAP_MAPBOX_API_KEY": "@web-district-map-mapbox-api-key"
    }
  },
  "env": {
    "NODE_MAILJET_KEY": "@web-mailjet-key",
    "NODE_MAILJET_SECRET": "@web-mailjet-secret",
    "NODE_MAILJET_SENDER": "webmaster@pcwa.net",
    "NODE_RECAPTCHA_SITE_KEY": "@web-recaptcha-site-key",
    "NODE_RECAPTCHA_SECRET_KEY": "@web-recaptcha-secret-key",
    "NODE_COSMIC_READ_ACCESS_KEY": "@web-cosmic-read-key",
    "NODE_COSMIC_WRITE_ACCESS_KEY": "@web-cosmic-write-key",
    "NODE_GOOGLE_CSE_CX": "@web-google-cse-cx",
    "NODE_GOOGLE_CSE_KEY": "@web-google-cse-key",
    "NODE_DARKSKY_API_KEY": "@web-darksky-api-key-dev",
    "NODE_REDISLABS_CACHE_PASSWORD": "@web-redislabs-cache-key",
    "NODE_MAILCHIMP_DC": "us16",
    "NODE_MAILCHIMP_USERNAME": "pcwawater",
    "NODE_MAILCHIMP_API_KEY": "@web-mailchimp-api-key",
    "NODE_MAILCHIMP_PRIMARY_LIST": "fefab158d1",
    "NODE_LAMBDA_URL": "https://dev-web.pcwa.net"
  },
  "builds": [
    {
      "src": "packages/www/package.json",
      "use": "@now/next"
    },
    { "src": "packages/api/src/routes/**/*.ts", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/api/forecast(.*)",
      "dest": "packages/api/src/routes/forecast/index.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "GET"
      }
    },
    {
      "src": "/api/search(.*)",
      "dest": "packages/api/src/routes/search/index.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/media(.*)",
      "dest": "packages/api/src/routes/cosmic/media.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/csv-data(.*)",
      "dest": "packages/api/src/routes/cosmic/csv-data.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/csv(.*)",
      "dest": "packages/api/src/routes/cosmic/csv.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/objects(.*)",
      "dest": "packages/api/src/routes/cosmic/objects.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/object-types(.*)",
      "dest": "packages/api/src/routes/cosmic/object-types.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net"
      }
    },
    {
      "src": "/api/cosmic/uploads(.*)",
      "dest": "packages/api/src/routes/cosmic/uploads.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/contact-us(.*)",
      "dest": "packages/api/src/routes/mail/contact-us.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/irrigation-controller-rebate(.*)",
      "dest": "packages/api/src/routes/mail/irrigation-controller-rebate.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/irrigation-efficiencies-rebate(.*)",
      "dest": "packages/api/src/routes/mail/irrigation-efficiencies-rebate.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/lawn-replacement-rebate(.*)",
      "dest": "packages/api/src/routes/mail/lawn-replacement-rebate.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/washing-machine-rebate(.*)",
      "dest": "packages/api/src/routes/mail/washing-machine-rebate.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/toilet-rebate(.*)",

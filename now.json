        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mail/account-contact-info(.*)",
      "dest": "packages/api/src/routes/mail/account-contact-info.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST, OPTIONS"
      }
    },
    {
      "src": "/api/mailchimp/campaigns(.*)",
      "dest": "packages/api/src/routes/mailchimp/campaigns.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "GET"
      }
    },
    {
      "src": "/api/mailchimp/subscribe(.*)",
      "dest": "packages/api/src/routes/mailchimp/subscribe.ts",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "POST,OPTIONS"
      }
    },
    {
      "src": "/api/mailchimp/update/(?<id>[^/]+)",
      "dest": "packages/api/src/routes/mailchimp/update.ts?userHashId=$id",
      "headers": {
        "cache-control": "s-maxage=1,stale-while-revalidate",
        "access-control-allow-origin": "https://www.pcwa.net",
        "access-control-allow-methods": "PUT,OPTIONS"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "packages/www/static/favicon.ico",
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800"
      }
    },
    {
      "src": "/rate-adjust-2018",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",
        "Location": "https://www.pcwa.net/services/rate-adjust"
      }
    },
    {
      "src": "/about-pcwa/board-minutes",
      "status": 301,
      "headers": {
        "cache-control": "maxage=0, s-maxage=172800",

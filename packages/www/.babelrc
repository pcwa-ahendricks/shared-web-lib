{
  "presets": ["next/babel", "@babel/preset-react"],
  "plugins": [
    "inline-react-svg",
    [
      "transform-assets-import-to-string",
      {
        "baseDir": "static",
        "baseUri": "/public"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": []
    },
    "test": {
      "plugins": ["babel-plugin-typescript-to-proptypes"]
    },
    "stage": {
      "plugins": ["babel-plugin-typescript-to-proptypes"]
    },
    "development": {
      "plugins": ["babel-plugin-typescript-to-proptypes"]
    }
  }
}

  "author": "Abe Hendricks",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "scripts": {
    "dev": "npm run next",
    "start": "npm run next",
    "next": "next",
    "build": "next build",
    "export": "next export",
    "serve": "serve ./out",
    "preserve": "npm run build && npm run export",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:stats": "cross-env STATS=1 next build && cat .next/stats.txt",
    "format": "prettier --write './**/*.js'",
    "type-check": "tsc -p .",
    "build:modernizr": "modernizr -q -c modernizr-config.json -d lib/modernizr.js"

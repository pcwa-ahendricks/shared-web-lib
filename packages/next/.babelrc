{
  "presets": ["next/babel"],
  "plugins": [
    "inline-react-svg",
    [
      "transform-assets-import-to-string",
      {
        "baseDir": "static",
        "baseUri": "/"
      }
    ],
    [
      "transform-imports",
      {
        "@material-ui/icons": {
          "transform": "@material-ui/icons/${member}",
          "preventFullImport": true
        },
        "mdi-material-ui": {
          "transform": "mdi-material-ui/${member}",
          "preventFullImport": true
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": []
    },
    "stage": {
      "plugins": ["babel-plugin-typescript-to-proptypes"]
    },
    "development": {
      "plugins": ["babel-plugin-typescript-to-proptypes"]
    }
  }
}

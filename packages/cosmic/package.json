{
  "name": "new-web-cosmic",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "dev",
    "watch": "babel src --watch --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "concurrently 'npm run watch' 'micro-dev -s -p 3003'",
    "inspect": "node --inspect node_modules/.bin/micro-dev",
    "test": "ava",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --declaration true --allowJs false",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\"  --ignore src/**/*.spec.js,src/**/*.test.js",
    "build:js-src-map": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "author": "<ahendricks@pcwa.net>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@pcwa/micro-check-referrer": "^0.1.13",
    "@pcwa/micro-limiter": "^0.1.13",
    "@pcwa/micro-no-cache": "^0.1.10",
    "@pcwa/micro-unauthorized": "^0.1.14",
    "busboy": "^0.3.1",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.0.0-beta.2",
    "dotenv-safe": "^6.1.0",
    "form-data": "^2.4.0",
    "http-status-codes": "^1.3.2",
    "isomorphic-unfetch": "^3.0.0",
    "jimp": "^0.6.4",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "micro-fork": "^2.0.0",
    "micro-middleware": "^0.1.0",
    "mime": "^2.4.4",
    "pretty-bytes": "^5.3.0",
    "querystringify": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/busboy": "^0.2.3",
    "@types/form-data": "^2.5.0",
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^12.7.0",
    "@types/querystringify": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "ava": "^2.1.0",
    "concurrently": "^4.1.1",

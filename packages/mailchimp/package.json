{
  "name": "neu-web-mailchimp",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "micro-dev -s -p 3005",
    "watch": "babel src --watch --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "npm-run-all -p watch start",
    "inspect": "node --inspect node_modules/.bin/micro-dev",
    "test": "ava",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --declaration true --allowJs false",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\"  --ignore src/**/*.spec.js,src/**/*.test.js",
    "build:js-src-map": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "author": "<ahendricks@pcwa.net>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@pcwa/micro-check-referrer": "^0.1.13",
    "@pcwa/micro-no-cache": "^0.1.10",
    "@pcwa/micro-unauthorized": "^0.1.14",
    "dotenv-safe": "^8.1.0",
    "http-status-codes": "^1.3.2",
    "isomorphic-unfetch": "^3.0.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "micro-fork": "^2.0.0",
    "micro-middleware": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "ava": "^2.4.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "micro-dev": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "test-listen": "^1.1.0",
    "typescript": "^3.5.2"
  }
}

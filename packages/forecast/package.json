{
  "name": "new-web-forecast",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "micro",
    "server": "node dist/server.js",
    "server:dev": "cross-env NODE_ENV=development concurrently 'npm run watch' 'nodemon dist/server.js'",
    "build": "babel src --out-dir dist --ignore src/**/*.spec.js,src/**/*.test.js",
    "watch": "babel src --watch --out-dir dist",
    "dev": "concurrently 'npm run watch' 'micro-dev -s -p 1337'",
    "inspect": "node --inspect node_modules/.bin/micro-dev",
    "test": "ava"
  },
  "author": "<ahendricks@pcwa.net>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "dotenv-safe": "^6.1.0",
    "forecast": "^0.3.2",
    "micro": "^9.3.3",
    "micro-cors": "^0.1.1",
    "query-string": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "ava": "^1.2.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "micro-dev": "^3.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "test-listen": "^1.1.0"
  }
}

{
  "name": "neu-web",
  "version": "1.0.0",
  "description": "New PCWA website",
  "main": "index.js",
  "repository": "pcwa.net",
  "author": "abe hendricks <ahendricks@pcwa.net>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "forecast:dev": "npm run server:dev --prefix packages/forecast",
    "mailjet:dev": "npm run dev --prefix packages/mailjet",
    "next:dev": "npm run dev --prefix packages/next",
    "dev": "concurrently -n next,forecast,mailjet 'npm run next:dev' 'npm run forecast:dev' 'npm run mailjet:dev'",
    "serve": "concurrently -n next 'npm run serve --prefix packages/next'",
    "prenow": "npm run build --prefix packages/forecast",
    "now": "now && now alias",
    "now-build": "next build",
    "flow": "flow"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^4.1.0",
    "flow-bin": "^0.93.0",
    "now": "^14.0.1"
  }
}
